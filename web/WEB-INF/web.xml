<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--只要将监听器注册给tomcat,tomcat就可以对其进行监听-->
    <listener>
        <listener-class>Listener.Servlet_Listener</listener-class>
    </listener>
    <!--只要将监听器注册给tomcat,tomcat就可以对其进行监听-->
    <servlet>
        <servlet-name>BMI_Servlet</servlet-name>
        <servlet-class>Test.BMI_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BMI2_Servlet</servlet-name>
        <servlet-class>Test.BMI2_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BMI03_Servlet</servlet-name>
        <servlet-class>Test.BMI03_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>queryProvince_Servlet</servlet-name>
        <servlet-class>Test.queryProvince_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>queryJson_Servlet</servlet-name>
        <servlet-class>Test.queryJson_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BMI_Servlet</servlet-name>
        <url-pattern>/bmi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BMI2_Servlet</servlet-name>
        <url-pattern>/bmi2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BMI03_Servlet</servlet-name>
        <url-pattern>/bmi03</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>queryProvince_Servlet</servlet-name>
        <url-pattern>/queryProvince</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>queryJson_Servlet</servlet-name>
        <url-pattern>/queryjson</url-pattern>
    </servlet-mapping>
</web-app>